<h1>Listing group_proposals</h1>

<table>
  <tr>
    <th>Group</th>
    <th>Proposal</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @group_proposals.each do |group_proposal| %>
  <tr>
    <td><%= group_proposal.group_id %></td>
    <td><%= group_proposal.proposal_id %></td>
    <td><%= link_to 'Show', group_proposal %></td>
    <td><%= link_to 'Edit', edit_group_proposal_path(group_proposal) %></td>
    <td><%= link_to 'Destroy', group_proposal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

