<h1>Listing project_statuses</h1>

<table>
  <tr>
    <th>Project</th>
    <th>Project status type</th>
    <th>Semester</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @project_statuses.each do |project_status| %>
  <tr>
    <td><%= project_status.project_id %></td>
    <td><%= project_status.project_status_type_id %></td>
    <td><%= project_status.semester_id %></td>
    <td><%= link_to 'Show', project_status %></td>
    <td><%= link_to 'Edit', edit_project_status_path(project_status) %></td>
    <td><%= link_to 'Destroy', project_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Project status', new_project_status_path %>
