<h1>Listing subheadings</h1>

<table>
  <tr>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @subheadings.each do |subheading| %>
  <tr>
    <td><%= subheading.title %></td>
    <td><%= link_to 'Show', subheading %></td>
    <td><%= link_to 'Edit', edit_subheading_path(subheading) %></td>
    <td><%= link_to 'Destroy', subheading, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Subheading', new_subheading_path %>
