<h1>Listing subheadings</h1>

<table>
  <tr>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @subheadings.each do |subheading| %>
  <tr>
    <td><%= subheading.title %></td>
    <td><%= link_to 'Show', page_subheading_path(@page,subheading) %></td>
    <td><%= link_to 'Edit', edit_page_subheading_path(@page, subheading) %></td>
    <td><%= link_to 'Destroy', page_subheading_path(@page, subheading), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Subheading', new_page_subheading_path %>
